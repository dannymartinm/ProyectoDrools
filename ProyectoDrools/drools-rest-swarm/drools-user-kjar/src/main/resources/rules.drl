package org.drools.workshop;
import org.drools.workshop.model.*;

rule "Recomendacion"
    when
        $a: Alumno()
        $ej: Ejercicio()
    then
        System.out.println("RULE");
        insert(new Recomendacion($a.getNombre(), $a.getMatricula()));
        insert(new Recomendacion($ej.getNombre(), $ej.getDescripcion()));
end